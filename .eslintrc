{
    "extends" : ["rallycoding"],
    "rules" : {
        "arrow-body-style": 0,
        "max-len": 0,
        "no-nested-ternary": 0,
        "no-underscore-dangle": 0,
        "prefer-template": 0,
        "global-require": 0,
        "import/no-unresolved": 0,
        "import/no-extraneous-dependencies": 0,
        "key-spacing": 0,
        "camelcase": 0,
        "padded-blocks": 0
    }
}
